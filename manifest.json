{
  "manifest_version": 3,
  "name": "GProxy",
  "version": "1.0",
  "description": "A lightweight proxy plug-in.",
  "permissions": [
    "proxy",
    "storage",
    "declarativeNetRequest"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
