{
  "manifest_version": 3,
  "name": "GProxy",
  "version": "2.0",
  "description": "A lightweight proxy plug-in.",
  "permissions": [
    "proxy",
    "storage",
    "declarativeNetRequest",
    "contextMenus"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "https://i.alal.site/*"
  ],
  "icons": {
    "16": "image/icon16.png",
    "48": "image/icon48.png",
    "128": "image/icon128.png"
  }
}
