{
  "manifest_version": 3,
  "name": "GProxy",
  "version": "4.0",
  "description": "A lightweight proxy plug-in.",
  "permissions": [
    "proxy",
    "storage",
    "declarativeNetRequest",
    "contextMenus",
    "history",
    "cookies",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "https://alal.site/*",
    "<all_urls>"
  ],
  "icons": {
    "16": "image/icon16.png",
    "48": "image/icon48.png",
    "128": "image/icon128.png"
  }
}
